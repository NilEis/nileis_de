---
import InlineScript from "./InlineScript.astro";
---

<script is:inline>
    MathJax = {
        loader: {
            paths: {
                mathjax: "https://cdn.jsdelivr.net/npm/mathjax@4.0.0",
            },
            load: ["input/tex", "input/mml", "input/asciimath", "output/svg", "output/chtml", "ui/menu"],
        },
        output: {
            font: "mathjax-newcm",
        },
        tex: {
            inlineMath: [
                ["$", "$"],
                ["\\(", "\\)"],
            ],
        },
    };
</script>
<InlineScript
    src="https://cdn.jsdelivr.net/npm/mathjax@4.0.0/startup.js"
    integrity="sha384-V8Uc+jzQMe7n4tFx1oAuCOiBj0WFbXurxmgghjXXzYHRKbpk8D/aCD16BkdE/MDh"
    credentials="omit"
    async
/>
